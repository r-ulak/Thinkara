<div class="container padding3centt" id="jobHistoryItemcontent-box">
    <div id="jobHistorycontent-wrapper" data-bind="foreach: jobCodes">
        <div class=" row">
            <div class="well well-sm">
                <div class="row padding3centt">
                    <div class="col-xs-3 col-md-3 text-center">

                        <div class="row padding3centt">
                            <div class="col-xs-12">
                                <i class="fa fa-5x text-info" data-bind="css: ImageFont"></i>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-9 section-box">
                        <div class="row">
                            <div class="col-xs-12">
                                <div class="col-xs-10">
                                    <h6>
                                        <label data-bind="text: Title"></label>
                                    </h6>
                                </div>
                                <div class="col-xs-2">
                                    <!-- ko if: OverTimeRate > 0 -->
                                    <span title="Allows OverTime" class="pull-right text-success fa fa-clock-o"></span>
                                    <!-- /ko -->
                                    <p class="text-success    fontsize70" data-bind="text:JobTypeName"></p>
                                </div>
                            </div>
                        </div>
                        <div class="row padding3centt">
                            <div class="col-xs-12">
                                <div class="col-xs-9 ">
                                </div>
                                <div class="col-xs-3">
                                    <!-- ko if: Status == 'P' -->
                                    <input type="checkbox" title="withdraw" class="pull-right" data-bind="attr:{'data-tid': TaskId}, checked: $root.jobHistoryWithdrawList, checkedValue:TaskId" />
                                    <!-- /ko -->
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row padding3centt">
                    <div class="col-xs-12">
                        <fieldset>
                            <legend class="text-warning fontsize120">
                                <!-- ko if: Status == 'A' -->
                                Accepted
                                <!-- /ko -->
                                <!-- ko if: Status == 'P' -->
                                Pending
                                <!-- /ko -->
                                <!-- ko if: Status == 'D' -->
                                Denied
                                <!-- /ko -->
                                <!-- ko if: Status == 'E' -->
                                Ended
                                <!-- /ko -->
                                <!-- ko if: Status == 'O' -->
                                Open Offer
                                <!-- /ko -->
                                <!-- ko if: Status == 'R' -->
                                Rejected
                                <!-- /ko -->
                                <!-- ko if: Status == 'Q' -->
                                Quit
                                <!-- /ko -->
                            </legend>
                            <div class="row">
                                <div class="col-xs-12">
                                    <div class="col-xs-6 fontsize80">
                                        <span>Salary(/hr)</span>
                                    </div>
                                    <div class="col-xs-6">
                                        <div class="pull-right" title="PurchasedPrice of degree">
                                            <i class="fa icon-money28  text-success fa-1x"></i>
                                            <span class="text-success fontsize80" data-bind="text:numeral(Salary).format('0,0.00a')">            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-xs-12">
                                    <div class="col-xs-6 fontsize80">
                                        <span>Year To Date</span>
                                    </div>
                                    <div class="col-xs-6">
                                        <div class="pull-right" title="PurchasedPrice of degree">
                                            <i class="fa icon-money28  text-success fa-1x"></i>
                                            <span class="text-success fontsize80" data-bind="text:numeral(IncomeYearToDate).format('0,0.00a')">            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- ko if: Status == 'A' ||  Status == 'Q' || Status == 'E'-->
                            <div class="row padding3centt">
                                <div class="col-xs-12">
                                    <div class="col-xs-6 fontsize80">
                                        <span> Started On</span>
                                    </div>
                                    <div class="col-xs-6">
                                        <div class="pull-right">
                                            <i class=" fa fa-clock-o"></i>
                                            <span class="pull-right paddingleft5px fontsize80" data-bind="text: ' ' + moment.utc(StartDate).local().format('MM/DD/YY HH:mm')"> </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-xs-12">
                                    <div class="col-xs-6 fontsize80">
                                        <span> Last Day At Work</span>
                                    </div>
                                    <div class="col-xs-6">
                                        <div class="pull-right">
                                            <i class=" fa fa-clock-o"> </i>
                                            <span class="pull-right paddingleft5px fontsize80" data-bind="text: moment.utc(EndDate).local().format('MM/DD/YY HH:mm')"> </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- /ko -->
                            <div class="row padding3centt">
                                <div class="col-xs-12">
                                    <div class="col-xs-6 fontsize80">
                                        <span>Applied On</span>
                                    </div>
                                    <div class="col-xs-6">
                                        <div class="pull-right" title="PurchasedPrice of degree">
                                            <i class="fa fa-clock-o text-success fa-1x"></i>
                                            <span class="text-success fontsize80" data-bind="text:moment.utc(AppliedOn).local().format('MM/DD/YY HH:mm')"> </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                       
                         </fieldset>
                    </div>
                </div>
                <hr />
                <div class="row">
                    <div class="col-xs-12">
                        <div class="col-xs-4">
                            <u>
                                <span class="fontsize90" title="total duration of job" data-bind="text:Duration">hrs</span>
                                <span class="fontsize70">hrs</span>
                            </u>
                            <p title="total duration of job" class="fontsize70">Duration</p>
                        </div>

                        <div class="col-xs-4">
                            <u>
                                <span class="fontsize90" title="hours paid for each week" data-bind="text:MaxHPW"></span>
                                <span class="fontsize70">hrs</span>
                            </u>
                            <p title="hours paid for each week" class="fontsize70">Weekly Hrs</p>

                        </div>
                        <div class="col-xs-4">
                            <div class="pull-right">
                                <u>
                                    <span class="fontsize90" title="Overtime logged on each checkin" data-bind="text:CheckInDuration"></span>
                                    <span class="fontsize70">hrs</span>
                                </u>
                                <p title="Overtime logged on each checkin" class="fontsize70">OverTime Session</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

