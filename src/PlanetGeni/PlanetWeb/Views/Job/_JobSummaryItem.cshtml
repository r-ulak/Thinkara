<div class="container padding3centt well hidden" id="jobSummarycontent-box">
    <div class="row ">
        <div class="col-xs-12">
            <div class="col-xs-4">
                <span class="fontsize90">Accepted</span>
            </div>
            <div class="col-xs-8  text-right">
                <span class="fa icon-handshake1 fa-2x text-success"></span>
                <span class="" data-bind="text: TotalJobsAccepted"></span>
            </div>
        </div>
    </div>
    <div class="row padding3centt">
        <div class="col-xs-12">
            <div class="col-xs-4">
                <span class="fontsize90">Rejcted</span>
            </div>
            <div class="col-xs-8  text-right">
                <span class="fa icon-dislike fa-2x text-danger"></span>
                <span class="" data-bind="text: TotalJobsRejcted"></span>
            </div>
        </div>
    </div>
    <div class="row padding3centt">
        <div class="col-xs-12">
            <div class="col-xs-5">
                <span class="fontsize90">Denied</span>
            </div>
            <div class="col-xs-7  text-right">
                <span class="fa  icon-denied1 fa-2x text-warning"></span>
                <span class="" data-bind="text: TotalJobsDenied"></span>
            </div>
        </div>
    </div>
    <div class="row padding3centt">
        <div class="col-xs-12">
            <div class="col-xs-5">
                <span class="fontsize90"> Open Offer</span>
            </div>
            <div class="col-xs-7  text-right">
                <span class="fa icon-man318 fa-2x text-success"></span>
                <span class="" data-bind="text: TotalJobsOpenOffer"></span>
            </div>
        </div>

    </div>
    <div class="row padding3centt">
        <div class="col-xs-12">
            <div class="col-xs-5">
                <span class="fontsize90"> Pending</span>
            </div>
            <div class="col-xs-7  text-right">
                <span class="fa icon-sand15 fa-2x text-info"></span>
                <span class="" data-bind="text: TotalJobsPending"></span>
            </div>
        </div>

    </div>

    <div class="row padding3centt">
        <div class="col-xs-12">
            <div class="col-xs-4">
                <span class="fontsize90"> Income YTD</span>
            </div>
            <div class="col-xs-8 text-right">
                <i class="fa icon-up74 fa-2x text-success"></i>
                <span class="fa-1x" data-bind="text:numeral(TotalIncomeYTD()).format('0,0.00')">   </span>
            </div>
        </div>
    </div>
    <div class="row padding3centt">
        <div class="col-xs-12">
            <div class="col-xs-4 ">
                <span class="fontsize90">Tax YTD</span>
            </div>
            <div class="col-xs-8 text-right" title="Summary Change">
                <i class="fa icon-ascending24 fa-2x text-success"></i>
                <span class="fa-1x" data-bind="text:numeral(TotalIncomeTaxYTD()).format('0,0.00')"></span>
            </div>
        </div>
    </div>
</div>
<div id="jobTopNSummary" class="well" data-bind="visible:jobTopN().length>0">
    @Html.Partial("_SummaryTop10")
</div>

