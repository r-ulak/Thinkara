<div id="jobsearchcontainer">
    <div class=" row">
        <div class="col-xs-12">
            <div class="panel panel-primary" id="myjobsearchcontent-box">
                <div class="panel-heading1 panel-heading">
                    <div class="flagsprite pull-right btn-link"
                         data-bind="css: 'flagsprite-' + CountryId().toLowerCase(), click: function(){viewCountryProfile(CountryId().toLowerCase())}"></div>

                    <span class="fa icon-online32 marginright6 "></span> Search Jobs
                </div>
                <div class="panel-body">
                    <div class="panel-group accordion" id="jobsearchAccordion">
                        <div class="panel panel-default">
                            <div class="panel-heading active">
                                <h4 class="panel-title">
                                    <a data-toggle="collapse" data-parent="#jobsearchAccordion" href="#jobSearch">
                                        <i class="fa fa-search fa-1x"></i>   Search
                                    </a>
                                </h4>
                            </div>
                            <div id="jobSearch" class="accordionjobsearch panel-collapse collapse in">
                                <div class="panel-body">
                                    @Html.Partial("_JobSearchItem")
                                </div>
                            </div>
                        </div>

                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h4 class="panel-title">
                                    <a data-toggle="collapse" data-parent="#jobsearchAccordion" href="#jobSearchResult">
                                        <i class="fa icon-online32 marginright6 fa-1x"></i> Search Result
                                    </a>
                                </h4>
                            </div>
                            <div id="jobSearchResult" class="accordionjobsearch panel-collapse collapse">
                                <div class="panel-body ">
                                    @Html.Partial("_JobSearchResultItem")
                                </div>
                                <div class="panel-footer">
                                    <div class="row ">
                                        <div class="col-xs-6">
                                            <h6>
                                                <span class="label label-info" data-bind="text: jobCodes().length"></span>
                                            </h6>
                                        </div>
                                        <div class="col-xs-3">
                                        </div>
                                        <div class="col-xs-3">
                                            <span id="jobCodesshowmore" class="marginright16 btn btn-shadow btn-success btn-sm pull-right"><i class="fa fa-plus"></i> </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-primary hidden" id="myjobsearchcontent-submit">
                <div class="panel-heading1 panel-heading">
                    <span>Job Apply</span>
                </div>
                <div class="panel-body">
                    <div class="col-xs-12">
                        <span data-bind="text:Message">

                        </span>
                    </div>
                </div>
            </div>

            <button class="btn btn-default backbtn pull-right hidden">
                <span><i class="fa fa-home"></i> Home</span>
            </button>
            <button class="btn btn-default  pull-left hidden" id="jobmorejobsearch">
                <span><i class="fa fa-search"></i> Search More</span>
            </button>
            <button type="submit" id="applyjobsearch" data-bind="disable: showApplyValidation()"
                    data-loading-text='
                <i class="fa fa-spinner fa-spin"></i> Saving...'
                class="btn btn-default pull-right">Apply For <span class="text-success" data-bind="text: applyingItems().length"></span> Job
            </button>
        </div>
    </div>
</div>
