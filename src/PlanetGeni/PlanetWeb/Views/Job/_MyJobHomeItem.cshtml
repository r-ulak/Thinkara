<div class="container padding3centt" id="jobMyItemcontent-box">
    <div id="jobMycontent-wrapper" data-bind="foreach: { data: jobCodes}">
        <div class="col-md-12 col-xs-12 productbox">
            <div class="row">
                <div class="col-xs-6">
                    <div class="pull-left">
                    </div><span class="fa fa-10x " data-bind="css: ImageFont"></span>
                </div>
                <div class="col-xs-6 ">
                    <div class="row">
                        <div class="col-xs-12 ">
                            <div class="pull-right">
                                <input type="text"
                                       data-bind="attr: {value: CompletionPercent, 'data-fgcolor': KnobColor,   'data-index': $index}"
                                       data-linecap="round"
                                       class="dial">
                                <p class="fontsize70">Completion Percent</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12">
                    <span class="text-info fa-2x" data-bind="text:Title"></span>
                </div>
            </div>
            <hr />
            <div class="row">
                <div class="col-xs-12">
                    <div class="col-xs-6 fontsize80">
                        <span>Salary(/hr)</span>
                    </div>
                    <div class="col-xs-6">
                        <div class="pull-right" title="PurchasedPrice of degree">
                            <i class="fa icon-money28  text-success fa-1x"></i>
                            <span class="text-success fontsize80" data-bind="text:numeral(Salary()).format('0,0.00a')">            </span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12">
                    <div class="col-xs-6 fontsize80">
                        <span>Year To Date</span>
                    </div>
                    <div class="col-xs-6">
                        <div class="pull-right" title="PurchasedPrice of degree">
                            <i class="fa icon-money28  text-success fa-1x"></i>
                            <span class="text-success fontsize80" data-bind="text:numeral(IncomeYearToDate()).format('0,0.00a')">            </span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12">
                    <div class="col-xs-6 fontsize80">
                        <span>Total Duration (hrs)</span>
                    </div>
                    <div class="col-xs-6">
                        <div class="pull-right">
                            <i class="fa fa-clock-o"></i>
                            <span class="pull-right paddingleft5px fontsize80" data-bind="text: Duration"> </span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12">
                    <div class="col-xs-6 fontsize80">
                        <span>Overtime Logged (hrs)</span>
                    </div>
                    <div class="col-xs-6">
                        <div class="pull-right">
                            <i class="fa fa-clock-o"></i>
                            <span class="pull-right paddingleft5px fontsize80" data-bind="text: OverTimeHours"> </span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row padding3centt">
                <div class="col-xs-12">
                    <div class="col-xs-6 fontsize80">
                        <span> Started On</span>
                    </div>
                    <div class="col-xs-6">
                        <div class="pull-right">
                            <i class=" fa fa-clock-o"></i>
                            <span class="pull-right paddingleft5px fontsize80" data-bind="text: ' ' + moment.utc(StartDate()).local().format('MM/DD/YY HH:mm')"> </span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12">
                    <div class="col-xs-6 fontsize80">
                        <span> Last Day At Work</span>
                    </div>
                    <div class="col-xs-6">
                        <div class="pull-right">
                            <i class=" fa fa-clock-o"> </i>
                            <span class="pull-right paddingleft5px fontsize80" data-bind="text: moment.utc(EndDate()).local().format('MM/DD/YY HH:mm')"> </span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12">
                    <div class="col-xs-6 fontsize80">
                        <span> Next Clock In At</span>
                    </div>
                    <div class="col-xs-6">
                        <div class="pull-right">
                            <i class="fa fa-clock-o"></i>
                            <span class="pull-right paddingleft5px fontsize80" data-bind="text: moment.utc(NextOverTimeCheckIn()).local().format('MM/DD/YY HH:mm')"> </span>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row padding3centt">
                <div class="col-xs-12">
                    <span class="fontsize70"> You will be paid in Overtime rate for cloking in, once you clock in you will be in on overtime clock for </span>
                    <span class="fontsize70" data-bind="text:CheckInDuration"></span>
                    <span class="fontsize70">hrs and paid at</span>
                    <span class="fontsize70" data-bind="text:OverTimeRate"></span>
                    <span class="fontsize70">times base salary.</span>

                </div>
            </div>
            <hr />
            <div class="row padding3centt">
                <div class="col-xs-12">
                    <div class="col-xs-6">
                        <input type="checkbox" class="pull-left" data-bind="attr:{'data-tid': TaskId}, checked: $root.quitingItems, checkedValue:TaskId" />
                    </div>
                    <div class="col-xs-6">
                        <div class="pull-right">
                            <!-- ko if:  moment.utc(NextOverTimeCheckIn())<= moment.utc()  -->
                            <button class="btn btn-default  pull-right degreeBoost "
                                    data-loading-text='
                                <i class="fa fa-clock-o text-success fa-spin"></i> Clocking In...'
                                data-bind="attr:{'data-index': $index}, click:$root.CheckInOverTime">
                                <span><i class="fa fa-clock-o text-success"></i> Clock In</span>
                            </button>
                            <!-- /ko -->
                            <!-- ko if:  moment.utc(NextOverTimeCheckIn())> moment.utc()  -->
                            <button class="btn btn-default  pull-right ">
                                <span><i class="fa fa-clock-o fa-spin text-success"></i> On OverTime</span>
                            </button>
                            <!-- /ko -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

