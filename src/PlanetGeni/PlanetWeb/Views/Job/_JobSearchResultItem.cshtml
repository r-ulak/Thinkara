<div class="container padding3centt" id="jobSearchResultItemcontent-box">
    <div id="jobSearchResultcontent-wrapper" data-bind="foreach: jobCodes">
        <div class=" row">
            <div class="well well-sm">
                <div class="row padding3centt">
                    <div class="col-xs-3 col-md-3 text-center">

                        <div class="row padding3centt">
                            <div class="col-xs-12">
                                <i class="fa fa-5x text-info" data-bind="css: ImageFont"></i>
                                <p></p>
                                <p class="fontsize70 pull-left " data-bind="text:IndustryName"></p>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-9 section-box">
                        <div class="row">
                            <div class="col-xs-12">
                                <div class="col-xs-10">
                                    <h6>
                                        <label data-bind="text: Title"></label>
                                    </h6>
                                </div>
                                <div class="col-xs-2">
                                    <!-- ko if: OverTimeRate > 0 -->
                                    <span title="Allows OverTime" class="pull-right text-success fa fa-clock-o"></span>
                                    <!-- /ko -->
                                    <p class="text-success    fontsize70" data-bind="text:JobTypeName"></p>
                                </div>
                            </div>
                        </div>
                        <div class="row padding3centt">
                            <div class="col-xs-12">
                                <div class="col-xs-9 ">
                                    <div class="">
                                        <span class="fa icon-money28 fa-1_5x  text-success"></span>
                                        <span class="text-success fa-1_5x" data-bind="text:Salary"></span>
                                        <p class="fontsize70  ">hourly</p>
                                    </div>
                                </div>
                                <div class="col-xs-3">
                                    <div></div>
                                    <input type="checkbox" class="pull-right" data-bind="attr:{'data-jid': JobCodeId}, checked: $root.applyingItems, checkedValue:JobCodeId" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row padding3centt">
                    <div class="col-xs-12">
                        <fieldset>
                            <legend class="text-warning fontsize120">
                                Qualification

                            </legend>

                            <p class="fontsize80">
                                At least <strong data-bind="text:IndustryExperience"></strong> months of experince in <strong data-bind="text:IndustryName"></strong> industry prefered.
                            </p>
                            <p class="fontsize80">
                                <strong data-bind="text:JobExperience"></strong> months of job experince in <strong data-bind="text:Title"></strong> prefered.
                            </p>
                            <p class="fontsize80">
                                <strong data-bind="text:ReqDegreeName"></strong> of  <strong data-bind="text:ReqMajorName"></strong>  requried.
                            </p>
                        </fieldset>
                    </div>
                </div>
                <hr />
                <div class="row">
                    <div class="col-xs-12">
                        <div class="col-xs-4">
                            <u>
                                <span class="fontsize90" title="total duration of job" data-bind="text:Duration">hrs</span>
                                <span class="fontsize70">hrs</span>
                            </u>
                            <p title="total duration of job" class="fontsize70">Duration</p>
                        </div>

                        <div class="col-xs-4">
                            <u>
                                <span class="fontsize90" title="hours paid for each week" data-bind="text:MaxHPW"></span>
                                <span class="fontsize70">hrs</span>
                            </u>
                            <p title="hours paid for each week" class="fontsize70">Weekly Hrs</p>

                        </div>
                        <div class="col-xs-4">
                            <div class="pull-right">
                                <u>
                                    <span class="fontsize90" title="Overtime logged on each checkin" data-bind="text:CheckInDuration"></span>
                                    <span class="fontsize70">hrs</span>
                                </u>
                                <p title="Overtime logged on each checkin" class="fontsize70">OverTime Session</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

