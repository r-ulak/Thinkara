<div class=" row">
    <div class="col-xs-12">
        <div class="well well-sm">
            <div class="row  padding3centb">
                <div class="col-xs-3 col-md-3 text-center">

                    <div class="row padding3centt">
                        <div class="col-xs-12">
                            <i class="fa fa-5x text-info" data-bind="css: ImageFont, title:IndustryName"></i>
                        </div>
                    </div>
                </div>
                <div class="col-xs-9 section-box">
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="col-xs-10">
                                <h6>
                                    <label data-bind="text: Title"></label>
                                </h6>
                                <small data-bind=" text:IndustryName"></small>
                            </div>
                            <div class="col-xs-2">
                                <!-- ko if: OverTimeRate > 0 -->
                                <span title="Allows OverTime" class="pull-right text-success fa fa-clock-o"></span>
                                <!-- /ko -->
                                <p class="text-success    fontsize70" data-bind="text:JobTypeName"></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row ">
                <div class="col-xs-12 ">
                    <div class="col-xs-4">
                        <u>
                            <i class="fa icon-money28"></i>
                            <span class="fontsize90" title="Salary" data-bind="text:numeral(Salary).format('0,0.00')"></span>
                        </u>
                        <p title="total duration of job" class="fontsize70">Salary/hr</p>
                    </div>
                    <div class="col-xs-4">
                        <u>
                            <!-- ko if: moment.utc(EndDate)> moment.utc() -->
                            <span class="fontsize90" title="Worked Hours" data-bind="text: moment.utc().diff(moment.utc(StartDate),'hours')"></span>
                            <!-- /ko -->
                            <!-- ko if: moment.utc(EndDate)<= moment.utc() -->
                            <span class="fontsize90" title="Worked Hours" data-bind="text: moment.utc(EndDate).diff(moment.utc(StartDate),'hours')"></span>
                            <!-- /ko -->
                            <span class="fontsize70">hrs</span>
                        </u>
                        <p title="hours paid for each week" class="fontsize70">Worked Hrs</p>
                    </div>
                    <div class="col-xs-4">
                        <div class="pull-right">
                            <u>
                                <i class="fa icon-money28"></i>
                                <span class="fontsize90" title="Income Year To Date" data-bind="text:numeral(IncomeYearToDate).format('0,0.00')"></span>
                            </u>
                            <p title="Income Year To Date" class="fontsize70">YTD</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
