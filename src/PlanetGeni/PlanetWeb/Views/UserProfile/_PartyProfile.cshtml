<div class=" row">
    <div class="col-xs-12">
        <div class="well well-sm">
            <div class="row  padding3centb">
                <div class="col-xs-3 col-md-3 text-center">
                    <div class="row padding3centt">
                        <div class="col-xs-12">
                            <!-- ko if: LogoPictureId.indexOf(".")==-1  -->
                            <i class="fa-3x text-info" data-bind="css: LogoPictureId, attr:{title: PartyName}"></i>
                            <!-- /ko -->
                            <!-- ko if: LogoPictureId.indexOf(".") != -1 -->
                            <img class="img-full100" data-bind="attr:{src: partypicContainer + LogoPictureId, title: PartyName}" />
                            <!-- /ko -->
                        </div>
                    </div>
                </div>
                <div class="col-xs-9 section-box">
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="col-xs-10">
                                <h6>
                                    <a  data-bind="click: function(){getPartyInfo(PartyId)}">
                                        <label class="btn-link " data-bind="text:PartyName"></label>
                                    </a>
                                </h6>
                                <!-- ko if: MemberType=="M"-->
                                <small>Member</small>
                                <!-- /ko -->
                                <!-- ko if: MemberType=="F"-->
                                <small>Founder</small>
                                <!-- /ko -->
                                <!-- ko if: MemberType=="C"-->
                                <small>CoFounder</small>
                                <!-- /ko -->
                            </div>
                            <div class="col-xs-2">
                                <div class="flagsprite pull-right btn-link"
                                     data-bind="css: 'flagsprite-' + CountryId.toLowerCase(), click: function(){viewCountryProfile(CountryId.toLowerCase())}"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row ">
                <div class="col-xs-12 ">
                    <div class="col-xs-4">
                        <u>
                            <!-- ko if: MemberEndDate==null  -->
                            <span class="fontsize80" data-bind="text: moment.utc().diff(moment.utc(MemberStartDate),'days')"></span>
                            <span class="fontsize80">...</span>
                            <!-- /ko -->
                            <!-- ko if: MemberEndDate!=null  -->
                            <span class="fontsize80" data-bind="text: moment.utc(MemberEndDate).diff(moment.utc(MemberStartDate),'days')"></span>
                            <!-- /ko -->
                        </u>
                        <p title="total duration of job" class="fontsize70">Duration (days)</p>
                    </div>
                    <div class="col-xs-4">
                        <u>
                            <i class="fa icon-money28"></i>
                            <span class="fontsize90" title="Salary" data-bind="text:numeral(TotalValue).format('0,0.00')"></span>
                        </u>
                        <p title="total duration of job" class="fontsize70">NetWorth</p>
                    </div>
                    <div class="col-xs-4">
                        <div class="pull-right">
                            <u>
                                <span class="fontsize90" title="Income Year To Date" data-bind="text:PartySize"></span>
                            </u>
                            <p title="Income Year To Date" class="fontsize70">PartySize</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
