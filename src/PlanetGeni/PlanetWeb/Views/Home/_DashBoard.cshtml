<div class="row padding3centb">
    <div class="col-xs-12 ">
        <div class="col-xs-6 paddingl8 paddingr8">
            <div class="tileSuccess tile-max text-center" data-url='@Url.Action("GetUserProfile", "UserProfile")' data-tile="22">
                <div class="row">
                    <div class="col-xs-12" id="nav-profile">
                        <div class="col-xs-2 padding3centt">
                            <button type="button" class="btn btn-circle btn-shadow "><i class="fa fa-2x" data-bind="css:getUserLevelAvatar(UserLevel() / 1000)"></i></button>
                        </div>
                        <div class="col-xs-10">
                            <h6 class="marginleft15">
                                Net Worth
                                <strong data-bind="text: numeral(NetWorth()).format('0,0.00'), attr:{title:numeral(NetWorth()).format('0,0.00a')},css:cashTextCss(NetWorth())"> </strong>
                            </h6>
                            <p class="text-right">
                                <i class="marginright6 goldcolor fa fa-star fa-2x"></i><span class="fontsize70" data-bind="text:UserLevel()"></span>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-xs-6 paddingl8 paddingr8">
            <div class="tileWarning tile-max text-center" data-url='@Url.Action("GetCountryProfile", "CountryProfile")' data-tile="14">
                <div class="row">
                    <div class="col-xs-12" id="nav-country">
                        <div class="col-xs-5 padding3centt">
                            <div class="flagavatar flag-wrapper">
                                <div class=" padding10centt flag flag-icon-background"
                                     data-bind="css: 'flag-icon-' + CountryId().toLowerCase()">
                                </div>
                            </div>
                        </div>
                        <div class="col-xs-7 padding10centt pull-left">
                            <span class="text-right" data-bind="text:getCountryName(CountryId()) "></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>

