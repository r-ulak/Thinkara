<div class="padding3centt">
    <hr />
    <div class="col-xs-12" id="eleFriends" data-bind="visible:runasSelected() =='I'">
        <div class="panel panel-default recipients">
            <div class="panel-heading">
                <span class="button-checkbox pull-right margintop-5">
                    <button type="button" class="btn btn-sm" data-color="default">All</button>
                    <input type="checkbox" id="elefriendall" class="hidden" />
                </span>
                <span class="colorwhite">
                    Request Nomination
                </span>
            </div>
            <div class="panel-body col-xs-12" id="electionfriends" data-bind="foreach: friendDetailsDTO">
                <div class="recipient col-xs-6" data-bind="attr: {title: FullName}, click: $parent.clickelectionfriends">
                    <i data-bind="css: 'user-status' + OnlineStatus" class="pull-left"></i>
                    <img class="img-check" width="28px" height="28px" data-bind="attr: {src: profilepicContainer+Picture , title: NameFirst }">
                    <span data-bind="text: FullName"></span>
                    <input type="checkbox" data-bind="value: FriendUserId" class="hidden">
                </div>
            </div>
            <div class="clearfix"></div>
            <div class="panel-footer fontsize80" id="electionfriendsFooter">
                <span data-bind="text: friendSelected().length"></span>
                <span class="text-muted"> friend(s) selected</span>
                <!-- ko if:  friendSelected().length < 10 -->
                <span class="text-danger fontsize90 pull-right">10 Nomaination Requried</span>
                <!-- /ko -->
            </div>
        </div>
    </div>

</div>
