<div class=" row">
    <div class="col-xs-12">
        <div class="row  padding3centb">
            <div class="col-xs-3 col-md-3 text-center">
                <div class="row padding3centt">
                    <div class="col-xs-12">
                        <i class="fa fa-3x text-success" data-bind="css: ImageFont"></i>
                    </div>
                </div>
            </div>
            <div class="col-xs-9 section-box">
                <div class="row">
                    <div class="col-xs-12">
                        <div class="col-xs-10">
                            <h5>
                                <label data-bind="text: Description"></label>
                            </h5>
                        </div>
                        <div class="col-xs-2">
                            <input type="text"
                                   data-bind="attr: {value: TotalPercent , 'data-fgcolor': dialColor(), 'data-index':$index()}"
                                   data-linecap="round"
                                   class="dial pull-right">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
