<div class="row hidden" id="countryprofileitemHeader">
    <div class="row text-success padding3centb">
        <div class="col-xs-12 text-right ">
            <div class="col-xs-3 ">
            </div>
            <div class="col-xs-9 countryProfilestat">
                <h4>
                    Budget: <i class="fa icon-money28 marginright6"></i>
                    <strong data-bind="text: numeral(TotalBudget()).format('0,0.00'), attr:{title:numeral(TotalBudget()).format('0,0.00a')}"> </strong>
                </h4>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12  text-right countryProfilestat">
            <div class="col-xs-3  emphasis">
            </div>
            <div class="col-xs-3  emphasis">
                <h4><strong data-bind="text:numeral(PopulationRank()).format('0o')">  </strong></h4>
                <p><small>Population</small></p>

            </div>
            <div class="col-xs-3 emphasis">
                <h4><strong data-bind="text:SecurityAsset"></strong></h4>
                <p><small>Security Asset</small></p>
            </div>
            <div class="col-xs-3 emphasis">
                <h4><strong data-bind="text:numeral(RichestCountryRank()).format('0o')"></strong></h4>
                <p><small>Richest</small></p>
            </div>
        </div>
    </div>
    <div class="row dividertop countryProfilestat">
        <div class="col-xs-12 ">
            <div class="col-xs-5">
                <div class="flagavatarprofile flag-wrapper profilepic-img">
                    <div class="padding10centt flag flag-icon-background" data-bind="attr: {title: getCountryName(CountryId()) }, css: 'flag-icon-' + CountryId().toLowerCase()">
                    </div>
                </div>
            </div>
            <div class="col-xs-4 padding10centt pull-left">
                <span class="text-left" data-bind="text:getCountryName(CountryId()) "></span>
            </div>
        </div>
    </div>
</div>