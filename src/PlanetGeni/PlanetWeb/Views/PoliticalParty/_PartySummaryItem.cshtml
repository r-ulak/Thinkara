<div class="container padding3centt well" id="partySummarycontent-box">
    <div class="row ">
        <div class="col-xs-12">
            <div class="col-xs-4">
                <span class="fontsize90">Party MemberShip</span>
            </div>
            <div class="col-xs-8  text-right">
                <span class="fa icon-candidate fa-1x"></span>
                <span class="" data-bind="text: TotalParties"></span>
            </div>
        </div>
    </div>
    <div class="row padding3centt">
        <div class="col-xs-12">
            <div class="col-xs-4">
                <span class="fontsize90">Total Donation</span>
            </div>
            <div class="col-xs-8  text-right">
                <span class="fa icon-money28  fa-1x"></span>
                <span class="" data-bind="text: numeral(TotalDonation()).format('0,0.00')"></span>
            </div>
        </div>
    </div>
    <hr />
    <fieldset data-bind="visible:PartyInvites().length>0">
        <legend class=" text-warning fontsize120">Invites</legend>
        <div data-bind="foreach: { data: PartyInvites}">
            <div class="row padding3centt">

                <div class="col-xs-12">
                    <div class="col-xs-5">
                        <!-- ko if: MemberType() == 'F' -->
                        <span class="fontsize90">Founder</span>
                        <!-- /ko -->
                        <!-- ko if: MemberType() == 'C' -->
                        <span class="fontsize90">Co Founder</span>
                        <!-- /ko -->
                        <!-- ko if: MemberType() == 'M' -->
                        <span class="fontsize90">Member</span>
                        <!-- /ko -->
                    </div>
                    <div class="col-xs-7  text-right">
                        <span class="fa fa-times-circle-o fa-1x"></span>
                        <span class="" data-bind="text: TotalCount"></span>
                    </div>
                </div>
            </div>
        </div>
    </fieldset>

</div>
<div id="partyTopNSummary" class="well">
    @Html.Partial("_SummaryTop10")
</div>

