<div class="container padding3centt" id="partyMyHistoryItemcontent-box">
    <div id="partyMyHistoryItemcontent-wrapper" data-bind="foreach:{ data:partyCodes, afterRender:$root.afterMyHistoryPartyRender}">
        <div class=" row marginbottom5">
            <div class="well well-sm margintop5">
                <div class="row">
                    <div class="col-xs-12 ">
                        <div class="col-xs-6">
                            <span class="fontsize80">Member For</span>
                        </div>
                        <div class="col-xs-6 text-right">
                            <span class="fontsize80" data-bind="text: moment.utc(MemberEndDate).diff(moment.utc(MemberStartDate),'months')"></span><span class="fontsize80"> months</span>
                        </div>
                    </div>
                </div>
                <hr />
                <div class="row padding3centt">
                    @Html.Partial("_PartyItemHeader")
                    <div class="col-xs-8 section-box">
                        @Html.Partial("_PartyItemBody")
                        <div class="row padding3centt">
                            <div class="col-xs-12">
                                <div class="col-xs-9 ">
                                    <div class="">
                                        <span class="fa icon-money28 fa-1x  text-success"></span>
                                        <span class="text-success fa-1x" data-bind="text:TotalValue"></span>
                                        <p class="fontsize70  ">Worth</p>
                                    </div>
                                </div>
                                <div class="col-xs-3">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <hr />
                @Html.Partial("_PartyItemFooter")
            </div>

            <ul class="navigation">
                <li>
                    <a data-bind="attr:{href:'#partymyHistoryagenda' + $index(),'data-index':$index},click:$root.clickgetagenda" data-toggle="tab">
                        <span class="fa fa-newspaper-o fa-2x"></span>
                        <p class="fontsize70">Agenda</p>
                    </a>
                </li>
                <li>
                    <a data-bind="attr:{href:'#partymembersmyHistory' + $index(),'data-index':$index}, click:$root.clickgetAllMembers" data-toggle="tab">
                        <span class="fa fa-users fa-2x "></span>
                        <p class="fontsize70">Members</p>
                    </a>
                </li>
                <li>
                    <a data-bind="attr:{href:'#partydonatemyHistory' + $index(),'data-index':$index}, click:$root.clickDonate" data-toggle="tab">
                        <span class="fa icon-donation2 fa-2x "></span>
                        <p class="fontsize70">Donate</p>
                    </a>
                </li>
            </ul>
            <div class="partymyHistory-body">
                <div class="tab-content">
                    <div data-bind="attr:{id:'partymyHistoryagenda' + $index()}" class="tab-pane ">
                        <div class="row padding3centt">
                            <div class="col-xs-12">
                                <div class="row">
                                    <div class="col-xs-1"></div>
                                    <div class="col-xs-11">
                                        <ul data-bind="foreach:PoliticalPartyAgenda" class="list-styled">
                                            <li>
                                                <span class="fontsize80" data-bind="text:Description">
                                                </span>

                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div data-bind="attr:{id:'partymembersmyHistory' + $index()}" class="tab-pane">
                        @Html.Partial("_MyPartyMembers")
                    </div>
                    <div data-bind="attr:{id:'partydonatemyHistory' + $index()}" class="tab-pane">
                        @Html.Partial("_MyPartyDonate")
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

