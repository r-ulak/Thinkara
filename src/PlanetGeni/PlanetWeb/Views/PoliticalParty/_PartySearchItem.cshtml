<div class="row padding3centt ">
    <div class="col-xs-12" id="partysize">
        <fieldset>
            <legend class="text-warning fontsize120">Party size</legend>
            <div class="row fontsize90">
                <div class="col-xs-12 ">
                    <div class="col-xs-6">
                        <div class="radio">
                            <label>
                                <input type="radio" name="partysize" data-lrange="1" data-hrange="100" value="1"> 1 -  100
                            </label>
                        </div>
                    </div>
                    <div class="col-xs-6">
                        <div class="radio">
                            <label>
                                <input type="radio" name="partysize" data-lrange="101" data-hrange="300" value="2">  101 - 300
                            </label>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row fontsize90">
                <div class="col-xs-12">
                    <div class="col-xs-6">
                        <div class="radio">
                            <label>
                                <input type="radio" name="partysize" data-lrange="301" data-hrange="500" value="3"> 301 - 500
                            </label>
                        </div>
                    </div>
                    <div class="col-xs-6">
                        <div class="radio">
                            <label>
                                <input type="radio" name="partysize" data-lrange="501" data-hrange="0" value="4">  501  - &#8734;
                            </label>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row fontsize90">
                <div class="col-xs-12">
                    <div class="col-xs-6">
                        <div class="radio">
                            <label>
                                <input type="radio" name="partysize" data-lrange="0" data-hrange="0" checked value="6"> Any
                            </label>
                        </div>
                    </div>
                </div>
            </div>
        </fieldset>
    </div>
</div>
