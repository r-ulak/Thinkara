<div class="container padding3centt" id="partyMyItemcontent-box">
    <div id="partyMyItemcontent-wrapper" data-bind="foreach:{ data:partyCodes, afterRender:$root.afterMyPartyRender}">
        <div class=" row marginbottom5">
            <div class="well well-sm margintop5">
                <div class="row">
                    <div class="col-xs-12 ">
                        <div class="col-xs-6">
                            <span class="fontsize80">Member Since</span>
                        </div>
                        <div class="col-xs-6 text-right">
                            <span class="fontsize80" data-bind="text: moment.utc(MemberStartDate).local().fromNow()"></span>
                        </div>
                    </div>
                </div>
                <hr />
                <div class="row padding3centt">
                    @Html.Partial("_PartyItemHeader")
                    <div class="col-xs-8 section-box">
                        @Html.Partial("_PartyItemBody")
                        <div class="row padding3centt">
                            <div class="col-xs-12">
                                <div class="col-xs-9 ">
                                    <div class="">
                                        <span class="fa icon-money28 fa-1x  text-success"></span>
                                        <span class="text-success fa-1x" title="Total Value" data-bind="text:numeral(TotalValue).format('0,0.00')"></span>
                                        <p class="fontsize70  ">Worth</p>
                                    </div>
                                </div>
                                <div class="col-xs-3">
                                    <div></div>
                                    <span title="Quit Party" data-bind="click:leaveParty" data-loading-text="Quitting..." class="btn btn-shadow btn-danger btn-sm pull-right"><i class="fa fa-power-off"></i> </span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <hr />
                @Html.Partial("_PartyItemFooter")
            </div>
            <ul class="navigation">
                <li class="">
                    <a data-bind="attr:{href:'#partymyagenda' + $index(),'data-index':$index}, click:$root.clickgetagenda" data-toggle="tab">
                        <span class="fa fa-newspaper-o fa-2x"></span>
                        <p class="fontsize70">Agenda</p>
                    </a>
                </li>
                <li>
                    <a data-bind="attr:{href:'#partymembersmy' + $index(),'data-index':$index}, click:$root.clickgetAllMembers" data-toggle="tab">
                        <span class="fa fa-users fa-2x "></span>
                        <p class="fontsize70">Members</p>
                    </a>
                </li>
                <li>
                    <a data-bind="attr:{href:'#partydonatemy' + $index(),'data-index':$index}, click:$root.clickDonate" data-toggle="tab">
                        <span class="fa icon-donation2 fa-2x "></span>
                        <p class="fontsize70">Donate</p>
                    </a>
                </li>
                <li>
                    <a data-bind="attr:{href:'#partyinvitemy' + $index(),'data-index':$index}, click:$root.clickPartyInvite" data-toggle="tab">
                        <span class="fa fa-envelope-o fa-2x "></span>
                        <p class="fontsize70">Invite</p>
                    </a>
                </li>
                <li>
                    <a data-bind="attr:{href:'#partymanagemy' + $index(),'data-index':$index}, click:$root.clickPartyManage" data-toggle="tab">
                        <span class="fa fa-wrench fa-2x "></span>
                        <p class="fontsize70">Manage</p>
                    </a>
                </li>
            </ul>
            <div class="partymy-body">
                <div class="tab-content">
                    <div data-bind="attr:{id:'partymyagenda' + $index()}" class="tab-pane ">
                        @Html.Partial("_PartyAgenda")
                    </div>
                    <div data-bind="attr:{id:'partymembersmy' + $index()}" class="tab-pane">
                        @Html.Partial("_MyPartyMembers")
                    </div>
                    <div data-bind="attr:{id:'partydonatemy' + $index()}" class="tab-pane">
                        @Html.Partial("_MyPartyDonate")
                    </div>
                    <div class="tab-pane" data-bind="attr:{'data-index': $index(),id:'partyinvitemy' + $index()}" data-class="clickPartyInvite">
                    </div>
                    <div data-bind="attr:{id:'partymanagemy' + $index()}" class="tab-pane">


                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

