<div class="container padding3centt" id="partyViewItemcontent-box">
    <div id="partyViewItemcontent-wrapper" data-bind="foreach:{ data:partyCodes}">
        <div class=" row marginbottom5">
            <div class="well well-sm margintop5">
                <div class="row padding3centt">
                    @Html.Partial("_PartyItemHeader")
                    <div class="col-xs-8 section-box">
                        @Html.Partial("_PartyItemBody")
                        <div class="row padding3centt">
                            <div class="col-xs-12">
                                <div class="col-xs-9 ">
                                    <div class="">
                                        <span class="fa icon-money28 fa-1x  text-success"></span>
                                        <span class="text-success fa-1x" data-bind="text:numeral(TotalValue).format('0,0.00')"></span>
                                        <p class="fontsize70  ">Worth</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <hr />
                @Html.Partial("_PartyItemFooter")
            </div>
            <ul class="navigation">
                <li class="">
                    <a data-bind="attr:{href:'#partyviewagenda' + $index()}, click:$root.clickgetagenda" data-toggle="tab">
                        <span class="fa fa-newspaper-o fa-2x"></span>
                        <p class="fontsize70">Agenda</p>
                    </a>
                </li>
                <li>
                    <a data-bind="attr:{href:'#partymembersview' + $index(),'data-index':$index}, click:$root.clickgetAllMembers" data-toggle="tab">
                        <span class="fa fa-users fa-2x "></span>
                        <p class="fontsize70">Members</p>
                    </a>
                </li>
            </ul>
            <div class="partyview-body">
                <div class="tab-content">
                    <div data-bind="attr:{id:'partyviewagenda' + $index()}" class="tab-pane ">
                        <div class="row padding3centt">
                            <div class="col-xs-12">
                                <div class="row">
                                    <div class="col-xs-1"></div>
                                    <div class="col-xs-11">
                                        <ul data-bind="foreach:PoliticalPartyAgenda" class="list-styled">
                                            <li>
                                                <span class="fontsize80" data-bind="text:Description">
                                                </span>

                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div data-bind="attr:{id:'partymembersview' + $index()}" class="tab-pane">
                        @Html.Partial("_PartyMembers")

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

