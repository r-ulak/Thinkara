<div id="myHistorypartycontainer">
    <div class=" row">
        <div class="col-xs-12">
            <div class="panel panel-primary" id="partyMyHistorycontent-box">
                <div class="panel-heading1 panel-heading">
                    <div class="flagsprite pull-right btn-link"
                         data-bind="css: 'flagsprite-' + CountryId().toLowerCase(), click: function(){viewCountryProfile(CountryId().toLowerCase())}"></div>
                    <span class="fa icon-candidate  marginright6"></span> Previous Parties
                </div>
                <div class="panel-body">
                    @Html.Partial("_MyHistoryPartyHomeItem")
                </div>
                <div class="panel-footer">
                    <div class="row padding3centt">
                        <div class="col-xs-6">
                            <span class="label label-info" title="total parties" data-bind="text: partyCodes().length"></span>
                        </div>
                        <div class="col-xs-3"></div>
                        <div class="col-xs-3">
                            <span id="partyhistoryrefresh" class="btn btn-shadow btn-success btn-sm pull-right"><i class="fa fa-refresh"></i> </span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
