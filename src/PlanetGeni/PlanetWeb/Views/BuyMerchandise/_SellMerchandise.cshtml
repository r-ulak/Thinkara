<div id="sellmerchandisecontainer">
    <div class=" row">
        <div class="col-xs-12">
            <div class="panel panel-primary" id="merchandiseInventorycontent-box">
                <div class="panel-heading1 panel-heading">
                    <div class="flagsprite pull-right btn-link"
                         data-bind="css: 'flagsprite-' + CountryId().toLowerCase(), click: function(){viewCountryProfile(CountryId().toLowerCase())}"></div>
                    <span class="fa icon-shop3 marginright6"></span> Sell Property

                </div>
                <div class="panel-body">
                    @Html.Partial("_SellMerchandiseItem")
                </div>
                <div class="panel-footer">
                    <div class="row fontsize80 padding3centt">
                        <div class="col-xs-6">
                            <strong>
                                BeforeTax
                            </strong>
                        </div>
                        <div class="col-xs-6">
                            <div class="pull-right">
                                <span class="fa icon-money28 text-success"></span>
                                <span class="label label-success text-right
                                  fontsize110" data-bind="text: numeral(amountBeforeTax()).format('0,0.00')">0.00</span>
                            </div>
                        </div>
                    </div>
                    <div class="row fontsize80 padding3centt">
                        <div class="col-xs-6">
                            <strong>
                                Tax@@
                            </strong>
                            <small data-bind="text:taxRate"></small>%
                        </div>
                        <div class="col-xs-6">
                            <div class="pull-right">
                                <span class="fa icon-money28 text-success"></span>
                                <span class="label label-success text-right
                                  fontsize110" data-bind="text: numeral(tax()).format('0,0.00')">0.00</span>
                            </div>
                        </div>
                    </div>
                    <hr />
                    <div class="row padding3centt fontsize90">
                        <div class="col-xs-6">
                            <strong> Total </strong>
                        </div>
                        <div class="col-xs-6">
                            <div class="pull-right">
                                <span class="fa icon-money28 text-success"></span>
                                <span class="label label-success text-right
                                  fontsize110" data-bind="text: numeral(Amount()).format('0,0.00'), attr:{title:numeral(Amount()).format('0,0.00a')}"></span>
                            </div>
                        </div>
                    </div>
                    <div class="row padding3centt">
                        <div class="col-xs-6">
                            <h6>
                                <span class="label label-info" data-bind="text: merchandiseCodes().length"></span>
                            </h6>
                        </div>

                        <div class="col-xs-6">
                            <span id="merchandiserefresh" class="btn btn-shadow btn-success btn-sm pull-right"><i class="fa fa-refresh"></i> </span>
                            <span id="merchandiseinvshowmore" class="marginright16 btn btn-shadow btn-success btn-sm pull-right"><i class="fa fa-plus"></i> </span>

                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-primary hidden" id="merchandiseInventorycontent-submit">
                <div class="panel-heading1 panel-heading">
                    <span>Buy</span>
                </div>
                <div class="panel-body">
                    <div class="col-xs-12">
                        <span data-bind="text:Message">

                        </span>
                    </div>
                </div>
            </div>

            <button class="btn btn-default backbtn pull-right hidden">
                <span><i class="fa fa-home"></i> Home</span>
            </button>
            <button class="btn btn-default  pull-left hidden" id="sellmoreproperty">
                <span><i class="fa fa-home"></i> Sell More</span>
            </button>
            <button type="submit" id="sellmerchandise" data-bind="disable: showSellValidation()"
                    data-loading-text='
                <i class="fa fa-spinner fa-spin"></i> Selling...'
                class="btn btn-default pull-right">Sell
            </button>
        </div>
    </div>
</div>
