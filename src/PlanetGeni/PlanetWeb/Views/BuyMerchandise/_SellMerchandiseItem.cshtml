<div class="container padding3centt" id="merchandiseInventoryItemcontent-box" data-bind="visisble:merchandiseCodes().length>0">
    <div id="merchandiseInventorycontent-wrapper" data-bind="foreach: { data: merchandiseCodes, afterRender: $root.AfterMerchandiseRenderAction}">
        <div class="col-md-12 col-xs-12 productbox">
            <div class="row">
                <div class="col-xs-6">
                    <div class="pull-left">
                        <span class="fa fa-info-circle fa-2x" data-toggle="popover" data-bind="attr:{'data-content':Description}"></span>
                    </div><span class="fa fa-10x " data-bind="css: ImageFont"></span>
                </div>
                <div class="col-xs-6 ">
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="pull-right" title="PurchasedPrice of property">
                                <i class="fa icon-money28  text-danger fa-1x"></i>
                                <span class="text-danger fa-1x" data-bind="text:numeral(PurchasedPrice).format('0,0.00a')">            </span>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="pull-right" title="Rental money you will yield">
                                <i class="fa icon-for4 text-success fa-1x">
                                </i><span class="text-success fa-1x" data-bind="text:numeral(RentalPrice).format('0,0.00a')">            </span>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="pull-right" title="Resale rate">
                                <i class="fa fa-recycle text-warning fa-1x"></i>
                                <span class="text-warning fa-1x" data-bind="text:numeral(ResaleRate).format('0,0.00a') + '%'"></span>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="pull-right" title="Resale Price">
                                <i class="fa fa-recycle text-success fa-1x"></i>
                                <span class="text-success fa-1x" data-bind="text:numeral(ResaleAt()).format('0,0.00a')"></span>
                            </div>
                        </div>
                    </div><div class="row">
                        <div class="col-xs-12">
                            <div class="pull-right" title="units">
                                <span class="text-success fa-1x" data-bind="text:Quantity"></span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-10">
                    <span class="text-info fa-2x" data-bind="text:Name"></span>
                </div>
                <div class="col-xs-2 ">
                    <div class="pull-right">
                        <span class="text-info fa-2x" data-bind="text:SellingUnit()"></span>
                    </div>
                </div>
            </div>

            <div class="row padding3centt">
                <div class="col-xs-12">
                    <input type="text" class="merchandisetype" value="0" data-slider-range="0,100" data-slider-step="2"
                           data-bind="attr:{'data-index': $index, 'data-typeid':MerchandiseTypeId, 'data-cost': PurchasedPrice}">
                </div>
            </div>
        </div>

    </div>
</div>

