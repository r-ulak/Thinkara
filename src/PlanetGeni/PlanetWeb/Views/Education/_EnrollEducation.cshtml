<div id="enrolleducationcontainer">
    <div class=" row">
        <div class="col-xs-12">
            <div class="panel panel-primary" id="myeducationcontent-box">
                <div class="panel-heading1 panel-heading">
                    <div class="flagsprite pull-right btn-link"
                         data-bind="css: 'flagsprite-' + CountryId().toLowerCase(), click: function(){viewCountryProfile(CountryId().toLowerCase())}"></div>

                    <span class="fa icon-graduation22 marginright6"></span> Enroll Degree
                </div>
                <div class="panel-body">
                    @Html.Partial("_EnrollEducationItem")
                </div>
                <div class="panel-footer">
                    <div class="row ">
                        <div class="col-xs-6">
                            <strong class="fontsize80">Enrolled Degree(s)</strong>
                        </div>
                        <div class="col-xs-6">
                            <div class="pull-right">
                                <span class="label label-info" data-bind="text: enrollingDegreeCodes().length"></span>
                            </div>
                        </div>
                    </div>
                    <div class="row padding3centt fontsize80">
                        <div class="col-xs-6">
                            <strong> Avaliable Cash </strong>
                        </div>
                        <div class="col-xs-6">
                            <div class="pull-right">
                                <span class="fa icon-money28 text-success"></span>
                                <span class="fontsize110" data-bind="text: numeral(AmountAvailable()).format('0,0.00')"></span>
                            </div>
                        </div>
                    </div>
                    <div class="row fontsize80 padding3centt">
                        <div class="col-xs-6">
                            <strong>
                                BeforeTax
                            </strong>
                        </div>
                        <div class="col-xs-6">
                            <div class="pull-right">
                                <span class="fa icon-money28 text-success"></span>
                                <span class="label label-success text-right
                                  fontsize110" data-bind="text: numeral(amountBeforeTax()).format('0,0.00')">0.00</span>
                            </div>
                        </div>
                    </div>
                    <div class="row fontsize80 padding3centt">
                        <div class="col-xs-6">
                            <strong>
                                Tax@@
                            </strong>
                            <small data-bind="text:taxRate"></small>%
                        </div>
                        <div class="col-xs-6">
                            <div class="pull-right">
                                <span class="fa icon-money28 text-success"></span>
                                <span class="label label-success text-right
                                  fontsize110" data-bind="text: numeral(tax()).format('0,0.00')">0.00</span>
                            </div>
                        </div>
                    </div>
                    <hr />
                    <div class="row padding3centt fontsize90">
                        <div class="col-xs-6">
                            <strong> Total</strong>
                        </div>
                        <div class="col-xs-6">
                            <div class="pull-right">
                                <span class="fa icon-money28 text-success"></span>
                                <span class="label label-success text-right
                                  fontsize110" data-bind="text: (numeral(Amount()).format('0,0.00'))"></span>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12">
                            <span class="fontsize90 pull-right text-warning padding3centt" data-bind="visible: ShowValidation()">
                                Not Enough Money to Enroll
                            </span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-primary hidden" id="myeducationcontent-submit">
                <div class="panel-heading1 panel-heading">
                    <span><i class="fa fa-graduation-cap marginright6"></i>Enroll</span>
                </div>
                <div class="panel-body">
                    <div class="col-xs-12">
                        <span data-bind="text:Message">

                        </span>
                    </div>
                </div>
            </div>

            <button class="btn btn-default backbtn pull-right hidden">
                <span><i class="fa fa-home"></i> Home</span>
            </button>
            <button class="btn btn-default  pull-left hidden" id="enrollmoreeducation">
                <span>Enroll More</span>
            </button>
            <button type="submit" id="saveeducation" data-bind="disable: showEnrollValidation()"
                    data-loading-text='
                <i class="fa fa-spinner fa-spin"></i> Saving...'
                class="btn btn-default pull-right">Enroll
            </button>
        </div>
    </div>
</div>
