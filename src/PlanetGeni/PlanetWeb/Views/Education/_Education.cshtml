<div id="edu-container">
    <ul class="nav nav-pills">
        <li class="active">
            <a data-toggle="tab" href="#edu-enrolleducation" class="active">Enroll</a>
        </li>
        <li>
            <a data-toggle="tab" href="#edu-enrolled">Enrolled</a>
        </li>
        <li>
            <a data-toggle="tab" href="#edu-completed">Completed</a>
        </li>
        <li>
            <a data-toggle="tab" href="#edu-summary">Summary</a>
        </li>

    </ul>
    <div class="tab-content">
        <div id="edu-enrolleducation" class="tab-pane fade in active padding3centt autoheight">
            @Html.Partial("_EnrollEducation")
        </div>
        <div id="edu-enrolled" class="tab-pane fade padding3centt autoheight">
            @Html.Partial("_EnrolledDegree")
        </div>
        <div id="edu-completed" class="tab-pane fade padding3centt autoheight">
            @Html.Partial("_CompletedDegree")
        </div>
        <div id="edu-summary" class="tab-pane fade padding3centt autoheight">
            @Html.Partial("_EducationSummary")
        </div>
    </div>
</div>


